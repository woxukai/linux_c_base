#include<stdio.h>>
#include<stdlib.h>
#include<string.h>

int main()
{
	char sText[]="ÄãºÃ";
	int iLen=strlen(sText);
	int i=0;
	for(i=0;i<iLen;i++)
	{
		printf("sText[%d]=%c\n",i,sText[i]);
		printf("sText[%d]=0x%02x\n",i,(unsigned char)sText[i]);
		printf("sText[%d]=0x%02h\n",i,(unsigned char)sText[i]);
	}
}
